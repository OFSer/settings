set show-all-if-ambiguous on
set show-all-if-unmodified on
set menu-complete-display-prefix on
set colored-completion-prefix on
set revert-all-at-newline on
set colored-stats on
set completion-query-items 0
set page-completions off
set mark-directories on
Tab: menu-complete
"w": unix-word-rubout
"[Z": menu-complete-backward
$if Gdb
	"s": "dashboard -layout source\n"
	"k": "dashboard -layout stack\n"
	"a": "dashboard -layout assembly\n"
	"h": "dashboard -layout history\n"
	"m": "dashboard -layout memory\n"
	"r": "dashboard -layout registers\n"
	"t": "dashboard -layout threads\n"
	"b": "dashboard -layout breakpoints\n"
	"v": "dashboard -layout variables\n"
$endif
